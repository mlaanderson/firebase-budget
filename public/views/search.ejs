<div data-dismissible="true" data-theme="a" class="ui-corner-all" style="min-width: 300px;">
    <div data-role="header" role="banner" data-theme="b" class="ui-header ui-bar-b">
        <h3>Search</h3>
    </div>
    <div role="main" class="ui-content" style="height: 100%;">
        <div class="ui-field-contain">
            <input data-inline="true" type="search" id="searchTextBox" name="search">
            <button type="button" id="searchButton" class="ui-btn ui-corner-all ui-icon-search ui-btn-icon-left ui-mini">Search</button>
        </div>
        <div id="searchTable" style="overflow-y: scroll;">
            <table class="ui-responsive table-stroke" style="width: 100%">
                    <thead>
                        <tr>
                            <th class="trDate" data-priority="1">Date</th>
                            <th class="trCategory" data-priority="2">Category</th>
                            <th class="trName" data-priority="3">Name</th>
                            <th class="trAmount" data-priority="4">Amount</th>
                        </tr>
                    </thead>
                    <tbody id="searchResults"></tbody>
            </table>
        </div>
    </div>
</div>
