<div data-role="popup" id="recurringEditor" data-theme="a" class="ui-corner-all">
    <div style="padding:10px 20px">
        <h3>Search</h3>
        <div class="ui-field-contain">
            <input type="search" id="searchTextBox">
            <button data-inline="true" type="button" id="searchButton" class="ui-btn ui-corner-all ui-icon-search ui-btn-icon-left ui-mini ui-btn-inline">Search</button>
        </div>
        <div id="searchTable" style="overflow-y: scroll;">
            <table class="ui-responsive table-stroke" style="width: 100%">
                    <thead>
                        <tr>
                            <th class="trDate" data-priority="1">Date</th>
                            <th class="trCategory" data-priority="2">Category</th>
                            <th class="trName" data-priority="3">Name</th>
                            <th class="trAmount" data-priority="4">Amount</th>
                        </tr>
                    </thead>
                    <tbody id="searchResults"></tbody>
            </table>
        </div>
    </div>
</div>
