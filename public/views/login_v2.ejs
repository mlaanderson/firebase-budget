<div data-dismissible="false" data-theme="b" class="ui-corner-all" style="min-width: 300px;">
    <div data-role="header" role="banner" data-theme="a" class="ui-header ui-bar-a">
        <h3>Please Sign In</h3>
    </div>
    <div role="main" class="ui-content">
        <div class="ui-field-contain">
            <label for="email" class="ui-hidden-accessible">Username:</label>
            <input type="email" id="email" value="" placeholder="username" data-theme="a">
        </div>
        <div class="ui-field-contain">
            <label for="password" class="ui-hidden-accessible">Password:</label>
            <input type="password" id="password" value="" placeholder="password" data-theme="a">
        </div>
        <div class="" id="errors">
        </div>
        <button type="button" id="btnSignIn" class="ui-btn ui-corner-all ui-shadow ui-btn-b ui-btn-icon-left ui-icon-check">Sign in</button>
        <button style="display: none" type="button" id="btnSignInGoogle" class="ui-btn ui-corner-all ui-shadow ui-btn-b ui-btn-icon-left ui-icon-check">Sign in with Google</button>
        <a href="signup" id="createAccount">Sign up for an Account</a><br/>
        <a href="forgot" id="forgotPassword">Forgot Password?</a>
    </div>
</div>
